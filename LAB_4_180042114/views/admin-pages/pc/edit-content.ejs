<!-- page content -->
<div class="right_col" role="main">
    <div class="">
      <div class="clearfix"></div>
      <div class="row">
        <div class="col-md-12 col-sm-12">
          <div class="x_panel">
            <div class="x_title">
              <h1>Programming Contest /<small>Register a new team</small></h1>
              <ul class="nav navbar-right panel_toolbox">
                <li>
                  <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                </li>
                <li class="dropdown">
                  <a
                    href="#"
                    class="dropdown-toggle"
                    data-toggle="dropdown"
                    role="button"
                    aria-haspopup="true"
                    aria-expanded="false"
                    ><i class="fa fa-wrench"></i
                  ></a>
                </li>
              </ul>
  
              <div class="clearfix"></div>
            </div>
            <div class="x_content">
              <br />
  
              <% let success="Team has been registered successfully!" %>
              <%if(error.length>0){if(error==success){%>
              <div class="alert alert-success"><%=error%></div>
              <%}else{%>
              <div class="alert alert-danger"><%=error%></div>
              <%}%><%}%>
              <% let {teamName,institution,coachName,coachEmail,coachContact,coachtshirt,teamLeaderName,teamLeaderEmail,teamLeaderContact,teamLeadertshirt,teamMember1Name,teamMember1Email,teamMember1Contact,teamMember1tshirt,teamMember2Name,teamMember2Email,teamMember2Contact,teamMember2tshirt,} = team %> 
                <% let postUrl = `/ProgrammingContest/edit/${team._id}` %> 
              <form
                method="POST"
                action=<%= postUrl %> 
                id="demo-form2"
                data-parsley-validate
                class="form-horizontal form-label-left"
              >
                <h3>Information Regarding the Team</h3>
                <div class="item form-group">
                  <div class="form-row">
                    <div class="col">
                      <input
                        type="text"
                        name="teamName"
                        id="teamName"
                        class="form-control"
                        placeholder="Team Name"
                        value = <%= teamName %> 
                      />
                    </div>
                    <div class="col">
                      <input
                        type="text"
                        name="institution"
                        id="institution"
                        class="form-control"
                        placeholder="Institution Name"
                        value = <%= institution %> 
                      />
                    </div>
                  </div>
                </div>
  
                <br />
                <h3>Information Regarding the Coach</h3>
                <div class="item form-group">
                  <div class="form-row">
                    <div class="col">
                      <input
                        type="text"
                        name="coachName"
                        id="coachName"
                        class="form-control"
                        placeholder="Name of the Coach"
                        value = <%= coachName %> 
                      />
                    </div>
                    <div class="col">
                      <input
                        type="text"
                        name="coachContact"
                        id="coachContact"
                        class="form-control"
                        placeholder="Contact No. of the Coach"
                        value = <%= coachContact %> 
                      />
                    </div>
                    <div class="col">
                      <input
                        type="text"
                        name="coachEmail"
                        id="coachEmail"
                        class="form-control"
                        placeholder="Email of the Coach"
                        value = <%= coachEmail %> 
                      />
                    </div>
                  </div>
                </div>
                <h6>T-Shirt size of the Coach</h6>
                <div class="item form-group">
                  <div class="form-row">
                    <div class="col">
                      <select
                        class="form-control"
                        id="coachtshirt"
                        name="coachtshirt"
                      >
                        <option selected value=<%= coachtshirt %> ><%= coachtshirt %></option>
                        <option value="M">Medium</option>
                        <option value="L">Large</option>
                        <option value="XL">Extra Large</option>
                      </select>
                    </div>
                  </div>
                </div>
  
                <br />
                <h3>Information Regarding Team Leader</h3>
                <div class="item form-group">
                  <div class="form-row">
                    <!--  <div class="col">
                      <input type="text" name=teamLeaderName" id=teamLeaderName"
                      class="form-control" placeholder="Name of the Team Leader"
                      />
                    </div> -->
                    <div class="col">
                      <input
                        type="text"
                        name="teamLeaderName"
                        id="teamLeaderName"
                        class="form-control"
                        placeholder="Name No. of the Team Leader"
                        value=<%= teamLeaderName %>
                      />
                    </div>
  
                    <div class="col">
                      <input
                        type="text"
                        name="teamLeaderContact"
                        id="teamLeaderContact"
                        class="form-control"
                        placeholder="Contact No. of the Team Leader"
                        value=<%= teamLeaderContact %>
                      />
                    </div>
                    <div class="col">
                      <input
                        type="text"
                        name="teamLeaderEmail"
                        id="teamLeaderEmail"
                        class="form-control"
                        placeholder="Email of the Team Leader"
                        value=<%= teamLeaderEmail %>
                      />
                    </div>
                  </div>
                </div>
                <h6>T-Shirt size of the Team Leader</h6>
                <div class="item form-group">
                  <div class="form-row">
                    <div class="col">
                      <select
                        class="form-control"
                        id="teamLeadertshirt"
                        name="teamLeadertshirt"
                      >
                        <option selected value=<%= teamLeadertshirt %>><%= teamLeadertshirt %></option>
                        <option value="M">Medium</option>
                        <option value="L">Large</option>
                        <option value="XL">Extra Large</option>
                      </select>
                    </div>
                  </div>
                </div>
  
                <br />
                <h3>Information Regarding Member-1</h3>
                <div class="item form-group">
                  <div class="form-row">
                    <div class="col">
                      <input
                        type="text"
                        name="teamMember1Name"
                        id="teamMember1Name"
                        class="form-control"
                        placeholder="Name of Member-1"
                        value=<%= teamMember1Name %>
                      />
                    </div>
                    <div class="col">
                      <input
                        type="text"
                        name="teamMember1Contact"
                        id="teamMember1Contact"
                        class="form-control"
                        placeholder="Contact No. of Member-1"
                        value=<%= teamMember1Contact %>
                      />
                    </div>
                    <div class="col">
                      <input
                        type="text"
                        name="teamMember1Email"
                        id="teamMember1Email"
                        class="form-control"
                        placeholder="Email of Member-1"
                        value=<%= teamMember1Email %>
                      />
                    </div>
                  </div>
                </div>
                <h6>T-Shirt size of Member-1</h6>
                <div class="item form-group">
                  <div class="form-row">
                    <div class="col">
                      <select
                        class="form-control"
                        id="teamMember1tshirt"
                        name="teamMember1tshirt"
                      >
                        <option selected value=<%= teamMember1tshirt %>><%= teamMember1tshirt %></option>
                        <option value="M">Medium</option>
                        <option value="L">Large</option>
                        <option value="XL">Extra Large</option>
                      </select>
                    </div>
                  </div>
                </div>
  
                <br />
                <h3>Information Regarding Member-2</h3>
                <div class="item form-group">
                  <div class="form-row">
                    <div class="col">
                      <input
                        type="text"
                        name="teamMember2Name"
                        id="teamMember2Name"
                        class="form-control"
                        placeholder="Name of Member-2"
                        value=<%= teamMember2Name %>
                      />
                    </div>
                    <div class="col">
                      <input
                        type="text"
                        name="teamMember2Contact"
                        id="teamMember2Contact"
                        class="form-control"
                        placeholder="Contact No. of Member-2"
                        value=<%= teamMember2Contact %>
                      />
                    </div>
                    <div class="col">
                      <input
                        type="text"
                        name="teamMember2Email"
                        id="teamMember2Email"
                        class="form-control"
                        placeholder="Email of Member-2"
                        value=<%= teamMember2Email %>
                      />
                    </div>
                  </div>
                </div>
  
                <h6>T-Shirt size of Member-2</h6>
                <div class="item form-group">
                  <div class="form-row">
                    <div class="col">
                      <select
                        class="form-control"
                        id="teamMember2tshirt"
                        name="teamMember2tshirt"
                      >
                        <option selected value=<%= teamMember2tshirt %>><%= teamMember2tshirt %></option>
                        <option value="M">Medium</option>
                        <option value="L">Large</option>
                        <option value="XL">Extra Large</option>
                      </select>
                    </div>
                  </div>
                </div>
  
                <div class="ln_solid"></div>
                <div class="item form-group">
                  <div class="col-md-6 col-sm-6 offset-md-3">
                    <button class="btn btn-primary" type="reset">Reset</button>
                    <button type="submit" class="btn btn-success">Update</button>
                  </div>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>